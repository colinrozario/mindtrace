# ============================================
# MindTrace Dashboard Client Configuration
# ============================================

# API Server URL
VITE_API_URL=http://localhost:8000

# WebSocket URL (usually same as API URL)
VITE_WS_URL=ws://localhost:8000

# ============================================
# Optional Configuration
# ============================================

# App Title
VITE_APP_TITLE=MindTrace

# Enable Debug Mode
VITE_DEBUG=false

# Map Provider (for GPS tracking)
# Options: openstreetmap, mapbox
VITE_MAP_PROVIDER=openstreetmap

# Mapbox Access Token (if using Mapbox)
# VITE_MAPBOX_TOKEN=your-mapbox-token-here
